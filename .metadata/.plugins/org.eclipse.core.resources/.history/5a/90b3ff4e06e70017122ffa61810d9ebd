package fr.m2i.thread;

public interface ContainerStatistique {
	
	public int getNbMoyenEntreesParSeconde();
	public int getNbMoyenSortiesParSeconde();
	
	public default int generateTime()
	{
		int randomTime = 1 + (int)(Math.random() * ((10 - 1) + 1)) * 1000;
		return randomTime;
	}
	
	public default void addEntree()
	{
	}

	
}
